{
    "name": "opensoft/json-streaming-parser",
    "description": "A psr-7 streaming parser for JSON in PHP",
    "keywords": ["json", "parser", "streaming", "psr-7"],
    "homepage": "https://github.com/opensoft/jsonstreamingparser",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.3",
        "symfony/framework-bundle": "~2.1|^3.0|^4.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "suggest": {
        "symfony/psr-http-message-bridge": "for converting Symfony2 request to stream with param converter",
        "zendframework/zend-diactoros": "for psr-7 implementation of streams"
    }
}
